{
    "manifest_version": 3,
    "name": "HibouNT",
    "version": "1.3.3",
    "description": "A minimalist, customizable new tab experience.",
    "icons": {
        "48": "images/hibount-dark-removebg.png",
        "128": "images/hibount-dark-removebg.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "images/hibount-dark-removebg.png"
    },
    "background": {
  "scripts": ["newTab-override.js"],
  "persistent": false
},
    "permissions": [
        "tabs"
    ],
    "host_permissions": [],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "script.js"
            ],
            "css": [
                "styles.css"
            ]
        }
    ],
  "browser_specific_settings": {
  "gecko": {
    "id": "hiboudevinfo@gmail.com",
    "strict_min_version": "109.0"
  }
}
}
